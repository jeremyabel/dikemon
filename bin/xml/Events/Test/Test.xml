	<!-- CONDITIONAL: Money -->
	<EVENT_test_conditional_money>
		<SEQUENCE>
			<CONDITIONAL>
				<TYPE>MONEY</TYPE>
				<VALUE>120</VALUE>
				<IF_TRUE>
					<SEQUENCE>
						<DIALOG>
							<SPEAKER></SPEAKER>
							<TEXT>YOU RICH!   [end]</TEXT>
						</DIALOG>
						<END></END>
					</SEQUENCE>
				</IF_TRUE>
				<IF_FALSE>
					<SEQUENCE>
						<DIALOG>
							<SPEAKER></SPEAKER>
							<TEXT>GET MORE MONEY!   [end]</TEXT>
						</DIALOG>
						<END></END>
					</SEQUENCE>
				</IF_FALSE>
			</CONDITIONAL>
			<DIALOG>
				<SPEAKER></SPEAKER>
				<TEXT>End of Conditional   [end]</TEXT>
			</DIALOG>
			<END></END>
		</SEQUENCE>
	</EVENT_test_conditional_money>

