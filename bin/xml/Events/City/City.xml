	<!-- Player talks to the ticket sailor from the front -->
	<EVENT_talk_ticket_sailor>
		<SEQUENCE>
			<CONDITIONAL>
				<TYPE>FLAG</TYPE>
				<VALUE>gave_boat_ticket</VALUE>
				
				<!-- Player has already given ticket sailor the ticket -->
				<IF_TRUE>
					<SEQUENCE>
						<DIALOG>
							<SPEAKER>Seaman Semen</SPEAKER>
							<TEXT>I already got your ticket,[delay 4] go on ahead!   [end]</TEXT>
						</DIALOG>
						<END></END>
					</SEQUENCE>
				</IF_TRUE>
				
				<!-- Player has not given the ticket sailor the ticket, and is told to talk to the sailor to his right, because framing -->
				<IF_FALSE>
					<SEQUENCE>
						<DIALOG>
							<SPEAKER>Seaman Semen</SPEAKER>
							<TEXT>Buddy,[delay 8] I ain't allowed to talk to anyone who isn't standing on the dock.[halt] Union rules and all that.[halt] Come stand to my right and talk to me.   [end]</TEXT>
						</DIALOG>
						<END></END>
					</SEQUENCE>
				</IF_FALSE>
			</CONDITIONAL>
			<DELAY>2</DELAY>
			<SET_FACING>
				<TARGET>Seaman Semen</TARGET>
				<FACING>RIGHT</FACING>
			</SET_FACING>
			<END></END>
		</SEQUENCE>
	</EVENT_talk_ticket_sailor>
	
	<!-- Player walks into the trigger next to the ticket sailor -->
	<EVENT_trigger_ticket_sailor>
		<SEQUENCE>
			<!-- Ticket Sailor stops the player and asks to see a ticket -->
			<DELAY>2</DELAY>
			<SHOW_EMOTE>
				<TARGET>Seaman Semen</TARGET>
				<EMOTE>EXCLAIM</EMOTE>
			</SHOW_EMOTE>
			<DELAY>10</DELAY>
			<DIALOG>
				<SPEAKER>Seaman Semen</SPEAKER>
				<TEXT>Hey!   [end]</TEXT>
			</DIALOG>
			<SET_FACING>
				<TARGET>PLAYER</TARGET>
				<FACING>LEFT</FACING>
			</SET_FACING>
			<DELAY>2</DELAY>
			<HIDE_EMOTE>
				<TARGET>Seaman Semen</TARGET>
			</HIDE_EMOTE>
			<DELAY>2</DELAY>
			<DIALOG>
				<SPEAKER>Seaman Semen</SPEAKER>
				<TEXT>You ain't allowed onto the docks without a ticket!  [end]</TEXT>
			</DIALOG>
			<YESNO_QUESTION>
				<QUESTION>Do you have a ticket?</QUESTION>
				
				<!-- Player answers yes, they do have a ticket -->
				<IF_YES>
					<SEQUENCE>
					
						<!-- Play semen nametag gag -->
						<DIALOG>
							<SPEAKER>PLAYER</SPEAKER>
							<TEXT>Yes,[delay 4] but oh bro,[delay 4] someone wrote "semen" on your nametag.    [end]</TEXT>
						</DIALOG>
						<SUB_SEQUENCE>ticket_sailor_nametag_gag</SUB_SEQUENCE>
						<DIALOG>
							<SPEAKER>PLAYER</SPEAKER>
							<TEXT>Ok,[delay 4] jeez,[delay 4] I'm sorry[delay 4].[delay 4].[delay 4].    [end]</TEXT>
						</DIALOG>
						<DELAY>4</DELAY>
						
						<!-- Check if player really has the Boat Ticket item -->
						<CONDITIONAL>
							<TYPE>ITEM</TYPE>
							<VALUE>Boat Ticket</VALUE>
							
							<!-- Yep, player really has the Boat Ticket item, so ticket sailor looks it over and lets the player pass -->
							<IF_TRUE>
								<SEQUENCE>
									<DIALOG>
										<SPEAKER>Seaman Semen</SPEAKER>
										<TEXT>Let me just look this over real quick...  [end]</TEXT>
									</DIALOG>
									<DELAY>4</DELAY>
									<SET_FACING>
										<TARGET>Seaman Semen</TARGET>
										<FACING>DOWN</FACING>
									</SET_FACING>
									<DELAY>42</DELAY>
									<SET_FACING>
										<TARGET>Seaman Semen</TARGET>
										<FACING>RIGHT</FACING>
									</SET_FACING>
									<DELAY>2</DELAY>
									<DIALOG>
										<SPEAKER>Seaman Semen</SPEAKER>
										<TEXT>Everything looks to be in order![halt] Enjoy your vacation!   [end]</TEXT>
									</DIALOG>
									<DELAY>4</DELAY>
									<SET_FACING>
										<TARGET>PLAYER</TARGET>
										<FACING>DOWN</FACING>
									</SET_FACING>
									
									<!-- Set flags and clean up -->
									<SET_FLAG>gave_boat_ticket</SET_FLAG>
									<TAKE_ITEM>Boat Ticket</TAKE_ITEM>
									<REMOVE_TRIGGER>triggerTicketSailor</REMOVE_TRIGGER>
									
									<END></END>
								</SEQUENCE>
							</IF_TRUE>
							
							<!-- No, the player doesn't really have the Boat Ticket item. Player tries to bribe ticket sailor with a $50 bill, and he gets upset and tells the player to get out -->
							<IF_FALSE>
								<SEQUENCE>
									<DIALOG>
										<SPEAKER>Seaman Semen</SPEAKER>
										<TEXT>Oh,[delay 2] thank you[delay 8]--   [end]</TEXT>
									</DIALOG>
									<SHOW_EMOTE>
										<TARGET>Seaman Semen</TARGET>
										<EMOTE>EXCLAIM</EMOTE>
									</SHOW_EMOTE>
									<DELAY>4</DELAY>
									<DIALOG>
										<SPEAKER>Seaman Semen</SPEAKER>
										<TEXT>Wait a minute!   [end]</TEXT>
									</DIALOG>
									<HIDE_EMOTE>
										<TARGET>Seaman Semen</TARGET>
									</HIDE_EMOTE>
									<DIALOG>
										<SPEAKER>Seaman Semen</SPEAKER>
										<TEXT>This is a fifty dollar bill!   [end]</TEXT>
									</DIALOG>
									<DIALOG>
										<SPEAKER>PLAYER</SPEAKER>
										<TEXT>Well?   [end]</TEXT>
									</DIALOG>
									<DIALOG>
										<SPEAKER>Seaman Semen</SPEAKER>
										<TEXT>You’ve got the wrong idea buddy,[delay 4] get out of here!   [end]</TEXT>
									</DIALOG>
									<SUB_SEQUENCE>ticket_sailor_get_out</SUB_SEQUENCE>
									<SUB_SEQUENCE>ticket_sailor_return</SUB_SEQUENCE>
									<END></END>
								</SEQUENCE>
							</IF_FALSE>
						</CONDITIONAL>
						<END></END>
					</SEQUENCE>
				</IF_YES>
				
				<!-- Player answered no, they do not have a ticket. Ticket sailor says they need one to pass, and tells them to get out -->
				<IF_NO>
					<SEQUENCE>
						
						<!-- Play semen nametag gag -->
						<DIALOG>
							<SPEAKER>PLAYER</SPEAKER>
							<TEXT>No,[delay 4] but oh bro,[delay 4] someone wrote "semen" on your nametag.    [end]</TEXT>
						</DIALOG>						
						<SUB_SEQUENCE>ticket_sailor_nametag_gag</SUB_SEQUENCE>
						
						<!-- Ticket sailor tells player to get out -->
						<DIALOG>
							<SPEAKER>Seaman Semen</SPEAKER>
							<TEXT>And if you don’t have a ticket,[delay 8] then I can’t let you through!   [end]</TEXT>
						</DIALOG>
						<DELAY>4</DELAY>
						<SUB_SEQUENCE>ticket_sailor_get_out</SUB_SEQUENCE>
						<DIALOG>
							<SPEAKER>Seaman Semen</SPEAKER>
							<TEXT>Now get outta here!   [end]</TEXT>
						</DIALOG>
						<SUB_SEQUENCE>ticket_sailor_return</SUB_SEQUENCE>
						<END></END>
					</SEQUENCE>
				</IF_NO>
			</YESNO_QUESTION>
			<END></END>
		</SEQUENCE>
	</EVENT_trigger_ticket_sailor>
	
