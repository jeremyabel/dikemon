				<SPEED>8</SPEED>
			</WALK>
			<WALK>
				<TARGET>Seaman Semen</TARGET>
				<DIRECTION>RIGHT</DIRECTION>
				<TILES>1</TILES>
				<SPEED>8</SPEED>
			</WALK>
			<SET_FACING>
				<TARGET>Seaman Semen</TARGET>
				<FACING>UP</FACING>
			</SET_FACING>
			<DELAY>6</DELAY>
			<WALK sync="TRUE">
				<TARGET>Seaman Semen</TARGET>
				<DIRECTION>UP</DIRECTION>
				<TILES>1</TILES>
				<SPEED>8</SPEED>
			</WALK>
			<WALK keepfacing="TRUE">
				<TARGET>PLAYER</TARGET>
				<DIRECTION>UP</DIRECTION>
				<TILES>1</TILES>
				<SPEED>8</SPEED>
			</WALK>
			<END></END>
