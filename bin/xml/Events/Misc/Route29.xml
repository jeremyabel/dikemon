<?xml version="1.0" encoding="UTF-8"?>
<EVENTS>

	<EVENT_map_setup>
		<SEQUENCE>
			<CONDITIONAL>
				<TYPE>FLAG</TYPE>
				<VALUE>started_intro_lab</VALUE>
				<IF_TRUE>
					<SEQUENCE>
						<REMOVE_TRIGGER>triggerOakIntro1</REMOVE_TRIGGER>
						<REMOVE_TRIGGER>triggerOakIntro2</REMOVE_TRIGGER>
						<END></END>
					</SEQUENCE>
				</IF_TRUE>
				<IF_FALSE><SEQUENCE><END></END></SEQUENCE></IF_FALSE>
			</CONDITIONAL>
			<CONDITIONAL>
				<TYPE>FLAG</TYPE>
				<VALUE>done_first_battle</VALUE>
				<IF_TRUE>
					<SEQUENCE>
						<SET_FLAG>done_with_mom_talking</SET_FLAG>
						<END></END>
					</SEQUENCE>
				</IF_TRUE>
				<IF_FALSE><SEQUENCE><END></END></SEQUENCE></IF_FALSE>
			</CONDITIONAL>

			<!-- Remove Team Vomit after the fight is done -->
			<CONDITIONAL>
				<TYPE>FLAG</TYPE>
				<VALUE>fought_team_vomit</VALUE>
				<IF_TRUE>
					<SEQUENCE>
						<REMOVE_NPC>Vomit Girl</REMOVE_NPC>
						<REMOVE_NPC>Vomit Guy</REMOVE_NPC>
						<REMOVE_NPC>Vomit Cat</REMOVE_NPC>
						<END></END>
					</SEQUENCE>
				</IF_TRUE>
				<IF_FALSE><SEQUENCE><END></END></SEQUENCE></IF_FALSE>
			</CONDITIONAL>
			<END></END>
		</SEQUENCE>
	</EVENT_map_setup>


