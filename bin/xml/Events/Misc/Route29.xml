	<!-- Exclaim emote before Oak enters -->
	<EVENT_oak_intro_player_surprise>
		<SEQUENCE>
			<SET_FLAG>started_intro_lab</SET_FLAG>
			<DELAY>4</DELAY>
			<SET_FACING>
