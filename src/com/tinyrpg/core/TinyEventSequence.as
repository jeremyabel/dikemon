weenCommand		: TinyTweenNPCCommand;		private var currentSpriteObject		: TinyFieldMapObjectSprite;		private var currentBattleCommand	: TinyTrainerBattleCommand;				public var parentSequence			: TinyEventSequence;		public var restoreControl			: Boolean = true;		public var eventXMLData  			: XML;		public var eventDepth	 			: int = 0;
